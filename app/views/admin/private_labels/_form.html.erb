<%= form_for [:admin, @private_label] do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :namespace %>
    <%= f.text_field :namespace %>
  </div>

  <div class="field">
    <%= f.label :domain %>
    <%= f.text_field :domain %>
  </div>

  <div class="field">
    <%= f.label :terms_of_service %>
    <%= f.text_area :terms_of_service %>
  </div>



  <div class="form-actions">
    <%= f.submit :save %>
  </div>

<% end %>
